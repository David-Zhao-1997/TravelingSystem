<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="beans.User" catalog="TravelingSystem" schema="dbo" table="Users">
        <id name="email">
            <column name="Email" length="100" sql-type="varchar(100)"/>
        </id>
        <property name="uPass">
            <column name="UPass" length="100" sql-type="varchar(100)"/>
        </property>
        <property name="uName">
            <column name="UName" length="100" not-null="true" sql-type="varchar(100)"/>
        </property>
        <property name="state">
            <column name="State" sql-type="int"/>
        </property>
        <property name="dor">
            <column name="DOR" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>