<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="beans.Manager" catalog="TravelingSystem" schema="dbo" table="Manager">
        <id name="mid">
            <column name="MID" sql-type="int"/>
        </id>
        <property name="mPass">
            <column name="MPass" length="100" sql-type="varchar(100)"/>
        </property>
    </class>
</hibernate-mapping>