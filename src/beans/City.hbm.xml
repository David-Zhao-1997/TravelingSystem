<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="beans.City" catalog="TravelingSystem" schema="dbo" table="City">
        <id name="cityId">
            <column name="CityID" sql-type="int"/>
        </id>
        <property name="cName">
            <column name="CName" length="100" sql-type="varchar(100)"/>
        </property>
    </class>
</hibernate-mapping>